# Production Environment Variables Example
NODE_ENV=production
NEXT_PUBLIC_SITE_URL=https://mysonai.com

# Database
DATABASE_URL=postgresql://mysonai:your_postgres_password@postgres:5432/mysonai

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI
OPENAI_API_KEY=your_openai_api_key

# Stripe
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Google Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Redis
REDIS_URL=redis://redis:6379

# Feature Flags
NEXT_PUBLIC_FEATURE_ASSISTANTS=true
NEXT_PUBLIC_FEATURE_DEMO=true
NEXT_PUBLIC_FEATURE_PRICING=true
NEXT_PUBLIC_FEATURE_BLOG=true
NEXT_PUBLIC_FEATURE_CONTACT=true
NEXT_PUBLIC_FEATURE_AUTH=true
NEXT_PUBLIC_FEATURE_SIGNUP=true
NEXT_PUBLIC_FEATURE_SIGNIN=true
NEXT_PUBLIC_FEATURE_DASHBOARD=true
NEXT_PUBLIC_FEATURE_BILLING=true
NEXT_PUBLIC_FEATURE_CHAT=true
NEXT_PUBLIC_FEATURE_VISUAL_GENERATION=true
NEXT_PUBLIC_FEATURE_CODE_GENERATION=true
NEXT_PUBLIC_FEATURE_VOICE_GENERATION=false
NEXT_PUBLIC_FEATURE_API=true
NEXT_PUBLIC_FEATURE_WEBHOOKS=true
NEXT_PUBLIC_FEATURE_ANALYTICS=true
NEXT_PUBLIC_FEATURE_NOTIFICATIONS=true
NEXT_PUBLIC_FEATURE_PRIVACY=true
NEXT_PUBLIC_FEATURE_TERMS=true
NEXT_PUBLIC_FEATURE_COOKIES=true
NEXT_PUBLIC_FEATURE_GDPR=true
NEXT_PUBLIC_FEATURE_DEBUG=false
NEXT_PUBLIC_FEATURE_MAINTENANCE=false
NEXT_PUBLIC_FEATURE_BETA=false
NEXT_PUBLIC_FEATURE_PERFORMANCE=true
NEXT_PUBLIC_FEATURE_ACCESSIBILITY=true
NEXT_PUBLIC_FEATURE_LAZY_LOADING=true
NEXT_PUBLIC_FEATURE_VIRTUAL_SCROLLING=true

# Monitoring
GRAFANA_PASSWORD=your_grafana_password

# Security
JWT_SECRET=your_jwt_secret
ENCRYPTION_KEY=your_encryption_key

# Email
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password

# CDN
CDN_URL=your_cdn_url

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
